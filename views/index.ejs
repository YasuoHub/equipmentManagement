                                <li>设备名称：<span>示波器</span></li>
                                <li>申请时间：<span>2018-03-21</span></li>
                                <li>领取时间：<span>2018-03-21 5:00pm</span></li>
                                <li>办理人：<span>zss</span></li>
                                <li>联系电话：<span>111</span></li>
                                <li>审批状态：<span>待通过</span></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">确定
                    </button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal -->
    </div>
    <!--归还模态框结束-->

    <!--建议模态框开始-->
    <div class="modal fade" id="suggestModal" tabindex="-1" role="dialog" aria-labelledby="suggestModalTitle" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;
                    </button>
                    <h4 class="modal-title" id="suggestModalTitle">
                        建议反馈
                    </h4>
                </div>
                <div class="modal-body">
                    <div class="input-group">
                        <div class="form-group">
                            <%suggests.forEach(function (suggest) {%>
                            <ul class="suggestList">
                                内容：
                                <p><%=suggest.content%></p>
                                回复：
                                <p><%=suggest.response%></p>
                            </ul>
                            <%})%>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">确定
                    </button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal -->
    </div>
    <!--建议模态框结束-->

    <!--写建议模态框开始-->
    <div class="modal fade" id="writeTreatyModal" tabindex="-1" role="dialog" aria-labelledby="writeTreatyModalTitle" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
